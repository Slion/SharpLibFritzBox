  <!-- Target to build Squirrel release -->
  <Target Name="AfterBuild" Condition=" '$(Configuration)' == 'Release'">
    <!-- Get our version number -->
    <GetVersionParts AssemblyPath="$(TargetPath)">
      <Output TaskParameter="MajorVersion" PropertyName="MajorVersionNumber" />
      <Output TaskParameter="MinorVersion" PropertyName="MinorVersionNumber" />
      <Output TaskParameter="BuildVersion" PropertyName="BuildVersionNumber" />
      <Output TaskParameter="FriendlyVersion" PropertyName="ReleaseFriendlyVersion" />
    </GetVersionParts>
    <!-- Call our powershell script that will generate our Squirrel release -->
    <Exec Command="powershell.exe –NonInteractive –ExecutionPolicy Unrestricted -file ..\..\Publish\DoSquirrelRelease.ps1 @(nuGetPackage->'%(fullpath)') @(squirrelPackage->'%(fullpath)') $(OutDir) $(ReleaseFriendlyVersion) ..\..\Publish\FritzBoxDemo.nuspec http://publish.slions.net/FritzBoxDemo/ FritzBoxDemo" />
  </Target>
